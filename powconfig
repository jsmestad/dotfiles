if [ -f "$rvm_path/scripts/rvm" ] && [ -f ".rvmrc" ]; then
  source "$rvm_path/scripts/rvm"
  source ".rvmrc"
fi

if [ $(command -v rbenv 2>&1) ]; then
  export PATH=$(rbenv root)/shims:$(rbenv root)/bin:$PATH
fi

export POW_EXT_DOMAINS="ngrok.com"

